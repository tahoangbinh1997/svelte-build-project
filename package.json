{
	"name": "my-app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prepare": "husky install",
		"lint": "eslint --max-warnings=0 \"./src/**/*.{js,ts,tsx,svelte}\"",
		"lint:fix": "eslint --fix \"./src/**/*.{js,ts,tsx,svelte}\"",
		"format": "prettier --write \"{public,src}/**/*.{css,html,js,ts,tsx,svelte}\"",
		"postinstall": "svelte-kit sync"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-netlify": "^1.0.0-next.77",
		"@sveltejs/adapter-node": "^1.0.0-next.90",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@zerodevx/svelte-toast": "^0.8.0",
		"carbon-components-svelte": "^0.70.6",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.46.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.1",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0-beta.1"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@types/google.maps": "^3.50.0",
		"aos": "^3.0.0-beta.6",
		"axios": "^0.27.2",
		"cookie": "^0.4.1",
		"dayjs": "^1.11.5",
		"postcss": "^8.4.16",
		"sass": "^1.54.6"
	}
}
